{
    "log": {
        "access": "/opt/v2/access.log",
        "error": "/opt/v2/error.log",
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": __PORT__,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "__UUID__"
                    }
                ]
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {}
        }
    ]
}
